{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "carousel-layout-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/carouselLayout/CarouselLayoutWebPart.js",
          "manifest": "./src/webparts/carouselLayout/CarouselLayoutWebPart.manifest.json"
        }
      ]
    },
    "grid-layout-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/gridLayout/GridLayoutWebPart.js",
          "manifest": "./src/webparts/gridLayout/GridLayoutWebPart.manifest.json"
        }
      ]
    },
    "filmstrip-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/filmstrip/FilmstripWebPart.js",
          "manifest": "./src/webparts/filmstrip/FilmstripWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CarouselLayoutWebPartStrings": "lib/webparts/carouselLayout/loc/{locale}.js",
    "GridLayoutWebPartStrings": "lib/webparts/gridLayout/loc/{locale}.js",
    "FilmstripWebPartStrings": "lib/webparts/filmstrip/loc/{locale}.js"
  }
}
