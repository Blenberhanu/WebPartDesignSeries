{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "grid-layout-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/grid/GridWebPart.js",
          "manifest": "./src/webparts/grid/GridWebPart.manifest.json"
        }
      ]
    },
    "filmstrip-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/filmstrip/FilmstripWebPart.js",
          "manifest": "./src/webparts/filmstrip/FilmstripWebPart.manifest.json"
        }
      ]
    },
    "carousel-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/carousel/CarouselWebPart.js",
          "manifest": "./src/webparts/carousel/CarouselWebPart.manifest.json"
        }
      ]
    },
    "compact-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/compact/CompactWebPart.js",
          "manifest": "./src/webparts/compact/CompactWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "GridWebPartStrings": "lib/webparts/grid/loc/{locale}.js",
    "FilmstripWebPartStrings": "lib/webparts/filmstrip/loc/{locale}.js",
    "CarouselWebPartStrings": "lib/webparts/carousel/loc/{locale}.js",
    "CompactWebPartStrings": "lib/webparts/compact/loc/{locale}.js"
  }
}
